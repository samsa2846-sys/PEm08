# Example environment variables for MotionCraft AI Analyzer
# Copy this file to .env and fill in your actual API keys

# DeepSeek API Configuration
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_API_URL=https://api.deepseek.com/v1/chat/completions

# Yandex Vision API Configuration
YANDEX_VISION_API_KEY=your_yandex_api_key_here
YANDEX_VISION_FOLDER_ID=your_yandex_folder_id_here
YANDEX_VISION_ENDPOINT=https://vision.api.cloud.yandex.net/vision/v1/batchAnalyze

# Server Configuration (optional)
API_HOST=0.0.0.0
API_PORT=8000

# Application Settings
APP_TITLE=MotionCraft AI Analyzer
APP_VERSION=1.0.0
